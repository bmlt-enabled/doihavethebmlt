        "eslint-config-prettier": "^10.0.0",
        "eslint-plugin-svelte": "^3.0.0",
        "gh-pages": "^6.1.1",
        "prettier": "^3.1.1",
        "prettier-plugin-svelte": "^3.2.6",
        "sass": "^1.71.0",
        "svelte": "^5.0.0",
        "svelte-check": "^4.0.0",
        "svelte-preprocess": "^6.0.0",
        "tslib": "^2.4.1",
